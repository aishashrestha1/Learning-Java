//Write a Java program to calculate the average value of array elements.

public class Main {
  public static void main(String[] args) {
   
   int x = 0;
   int y = 0;
  int [] ary = new int [5];
  ary[0] = 1;
  ary[1] = 1;
  ary[2] = 5;
  ary[3] = 10;
  ary[4] = 3;
  

  for(int i=0; i < ary.length; i++){

 y= ary[i];
 x = y+x;
  }  
  int a =ary.length;
  System.out.println(x/a);
}
}
